<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="ar.edu.unq.epers.aterrizar.modelo.CategoriaTurista" table="categoria_turista" >
        
        <id name="id" type="java.lang.Integer">
            <column name="ID_CATEGORIA_TURISTA" />
            <generator class="identity" />
        </id>

        <property name="tipo" type="string">
            <column name="TIPO_CATEGORIA" length="20"/>
        </property>
        
        <property name="factor_de_precio" type="int">
            <column name="FACTOR_DE_PRECIO"/>
        </property>
                               
    </class>
</hibernate-mapping>